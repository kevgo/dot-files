{
  "breadcrumbs.enabled": false,
  "cucumberautocomplete.skipDocStringsFormat": true,
  "debug.javascript.suggestPrettyPrinting": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderIndicators": false,
  "editor.accessibilitySupport": "off",
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingQuotes": "beforeWhitespace",
  "editor.bracketPairColorization.enabled": true,
  "editor.colorDecorators": false,
  "editor.cursorBlinking": "solid",
  "editor.cursorSurroundingLines": 45,
  "editor.dragAndDrop": false,
  "editor.emptySelectionClipboard": false,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": false,
  "editor.guides.bracketPairs": true,
  "editor.guides.bracketPairsHorizontal": false,
  "editor.guides.highlightActiveBracketPair": false,
  "editor.guides.highlightActiveIndentation": false,
  "editor.hover.sticky": false,
  "editor.largeFileOptimizations": false,
  "editor.linkedEditing": false,
  "editor.minimap.enabled": false,
  "editor.multiCursorMergeOverlapping": false,
  "editor.quickSuggestions": {
    "comments": true,
    "other": true,
    "strings": true
  },
  "editor.renderControlCharacters": false,
  "editor.renderWhitespace": "none",
  "editor.roundedSelection": false,
  "editor.scrollBeyondLastLine": false,
  "editor.selectionHighlight": true,
  "editor.suggest.insertMode": "replace",
  "editor.suggest.showStatusBar": true,
  "editor.suggest.preview": true,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.occurrencesHighlight": true,
  "editor.tabSize": 2,
  "errorLens.fontStyleItalic": true,
  "errorLens.followCursor": "activeLine",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.decorations.badges": false,
  "extensions.ignoreRecommendations": true,
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "files.hotExit": "off",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.ignoreLimitWarning": true,
  "git.ignoreMissingGitWarning": false,
  "go.formatTool": "goimports",
  "go.toolsManagement.autoUpdate": true,
  "go.useLanguageServer": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "dprint.dprint",
  },
  "[json]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "json.schemas": [
    {
      "fileMatch": ["/masa.jsonc"],
      "url": "./masa.schema.json"
    }
  ],
  "[jsonc]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "keyboard.dispatch": "keyCode",
  "[markdown]": {
    "editor.occurrencesHighlight": false,
    "editor.selectionHighlight": false,
    "editor.defaultFormatter": "dprint.dprint",
    "editor.autoClosingBrackets": "never"
  },
  "markdown.extension.toc.levels": "2..6",
  "markdownIDE.autocompleteTitleRegex": "\\(([A-Z][A-Z0-9a-z]*[A-Z0-9])\\)$",
  "prettier.proseWrap": "always",
  "prettier.semi": false,
  "prettier.arrowParens": "avoid",
  "prettier.useEditorConfig": false,
  "prettier.ignorePath": "",
  "python.formatting.provider": "black",
  "redhat.telemetry.enabled": false,
  "rust-analyzer.assist.importGranularity": "module",
  "rust-analyzer.cache.warmup": false,
  "rust-analyzer.inlayHints.parameterHints": false,
  "rust-analyzer.inlayHints.typeHints": false,
  "rust-analyzer.lens.implementations": false,
  "[rust]": {
    "editor.detectIndentation": true,
    "editor.tabSize": 4
  },
  "scm.diffDecorations": "none",
  "search.smartCase": true,
  "[typescript]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vim.camelCaseMotion.enable": true,
  "vim.changeWordIncludesWhitespace": true,
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-b>": false,
    "<C-c>": false,
    "<C-t>": false
  },
  "vim.hlsearch": true,
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": ["<Esc>"],
      "after": ["<Esc>"],
      "commands": ["workbench.action.files.save"]
    }
  ],
  "vim.leader": ",",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "after": ["i", "<space>", "<esc>"],
      "before": ["<space>"]
    },
    {
      "after": ["o", "<esc>", "k"],
      "before": ["<enter>"]
    },
    {
      "after": ["l"],
      "before": ["s"]
    },
    {
      "after": ["s"],
      "before": ["l"]
    },
    {
      "before": ["<leader>", "/"],
      "commands": [":nohl"]
    },
    {
      "before": [":"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["<leader>", "l"],
      "commands": ["editor.action.openLink"]
    }
  ],
  "window.enableMenuBarMnemonics": false,
  "window.title": "${rootName}",
  "window.titleBarStyle": "custom",
  "workbench.activityBar.visible": false,
  "workbench.colorCustomizations": {
    "[Default Light+]": {
      "editor.lineHighlightBackground": "#f4f4ff"
    }
  },
  "workbench.editor.decorations.badges": false,
  "workbench.editor.revealIfOpen": true,
  "workbench.list.smoothScrolling": true,
  "workbench.list.keyboardNavigation": "simple",
  "workbench.quickOpen.closeOnFocusLost": false,
  "workbench.startupEditor": "none",
  "workbench.colorTheme": "Default Light+",
  "window.zoomLevel": -1
}
